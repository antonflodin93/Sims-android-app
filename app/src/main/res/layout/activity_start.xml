<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingStart="45dp"
    android:paddingTop="45dp"
    tools:context="se.miun.android_app.StartActivity">

    <Button
        android:id="@+id/startEmployeeUnitAcvitivy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:padding="30dp"
        android:text="Employeeunit"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="87dp" />

    <Button
        android:id="@+id/startMasterUnitAcvitivy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="30dp"
        android:padding="30dp"
        android:layout_marginBottom="30dp"
        android:text="Masterunit"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="87dp" />

    <Button
        android:id="@+id/startMainAcvitivy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="30dp"
        android:padding="30dp"
        android:layout_marginBottom="30dp"
        android:text="Main Activity"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="87dp" />



</LinearLayout>
